  <form action="/books/store" method="get" id="form">
    <input type="hidden" name="dataId" value="" id="id">
    <input type="hidden" name="status" value="<%= params[:status] %>">
    <div class="contains">
      <% @fj.each do |book| %>
      <div class="card">
        <div class="card-body d-flex list-inline">
          <% if book['volumeInfo']['imageLinks'].nil? %>
          <% else %>
          <div class="card-body-left">
            <%= image_tag(book['volumeInfo']['imageLinks']['smallThumbnail'], height: '60', width: '45', data: {"id" => book['id']}) %>
          </div>
          <% end %>
          <div class="card-body-right">
            <u><%= book['volumeInfo']['title'] %></u> (<%= book['volumeInfo']['authors'].join(', ') %>)
          </div>
        </div>
      </div>
      <% end %>
    </div>
  </form>




